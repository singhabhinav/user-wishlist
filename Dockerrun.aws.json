{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [{
            "name": "redis",
            "image": "redis:6.0.8",
            "hostname": "redis",
            "essential": false,
            "memory": 100
        },
        {
            "name": "db",
            "image": "postgres:12-alpine",
            "hostname": "db",
            "essential": false,
            "memory": 100,
            "environment": [{
                "POSTGRES_USER": "hello_flask",
                "POSTGRES_PASSWORD": "hello_flask",
                "POSTGRES_DB": "hello_flask_dev"
            }],
            "mountPoints": [{
                "sourceVolume": "postgres_data",
                "containerPath": "/var/lib/postgresql/data/"
            }]
        },
        {
            "name": "web",
            "image": "vagdevik/user-wishlist",
            "hostname": "web",
            "essential": true,
            "portMappings": [{
                "hostPort": 80,
                "containerPort": 80
            }],
            "links": ["redis", "db"],
            "memory": 100,
            "environment": [{
                "PYTHONUNBUFFERED": 1,
                "PYTHONDONTWRITEBYTECODE": 1
            }]
        }
    ]
}